{
  "manifest_version": 2,

  "name": "MgTokenRetrieval",
  "description": "Used to retrieve API token from minergate.com",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "clipboardWrite",
    "https://minergate.com/"
  ]
}
